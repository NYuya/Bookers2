



			<% if @users.present? %>
              <% @users.each do |user| %>
                <tr>
                  <td><%= user.name %></td>
                  <td><%= user.profile_image %></td>
                  <td><%= link_to "Show", user_path(user) %></td>
                </tr>
              <% end %>
            <% end %>
